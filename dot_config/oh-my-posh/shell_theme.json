{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "",
          "template": "{{ .Icon }} {{ .HostName }} ",
          "foreground": "#458588",
          "background": "#282828",
          "type": "os"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "{{ if .SSHSession }} ssh{{ end }}",
          "foreground": "#ebdbb2",
          "background": "#d65d0e",
          "type": "session"
        },
        {
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "/",
            "home_icon": "󰋜",
            "style": "full"
          },
          "style": "powerline",
          "template": " <#282828> 󰁔</> {{ .Path }} ",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#b16286",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": " <#ebdbb2> </>",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "cache": {
            "duration": "1minute",
            "strategy": "folder"
          },
          "style": "powerline",
          "template": "{{ .HEAD }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} 󰆓 {{ .StashCount }}{{ end }} ",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#965c6e",
          "type": "git"
        },
        {
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }} {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} ",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#7e4d5d",
          "type": "project"
        },
        {
          "style": "powerline",
          "template": "  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#689d6a",
          "type": "node"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " 󰔟 {{ .FormattedMs }} ",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#678881",
          "type": "executiontime"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "powerline",
          "template": "  {{ .CurrentDate | date .Format }}",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#458588",
          "type": "time"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "trailing_diamond": "",
          "template": " {{ if gt .Code 0 }}󰀦{{ else }}{{ end }} ",
          "foreground": "#ebdbb2",
          "powerline_symbol": "",
          "background": "#458588",
          "type": "exit",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc241d></> ",
            "yarn_icon": " <#458588>󰄛</>"
          },
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#458588",
          "background": "#282828",
          "type": "node"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#ebdbb2",
          "background": "#458588",
          "type": "python"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#fabd2f",
          "background": "#458588",
          "type": "java"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{ if .Unsupported }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#458588",
          "background": "#282828",
          "type": "dotnet"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": "󰟓 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#458588",
          "background": "#ebdbb2",
          "type": "go"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#985117",
          "background": "#ebdbb2",
          "type": "rust"
        },
        {
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": " ",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#458588",
          "background": "#ebdbb2",
          "type": "dart"
        },
        {
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": " ",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#cc241d",
          "background": "#ebdbb2",
          "type": "angular"
        },
        {
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": " ",
          "template": "<#cc241d></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#98971a",
          "background": "#b16286",
          "type": "julia"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#cc241d",
          "background": "#ebdbb2",
          "type": "ruby"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": "<#fabd2f></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#458588",
          "background": "#ebdbb2",
          "type": "azfunc"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "template": " {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "#fabd2f",
          "background": "#689d6a",
          "type": "aws"
        },
        {
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "󰠳 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "foreground": "#ebdbb2",
          "background": "#458588",
          "type": "kubectl"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "󰈸",
          "foreground": "#fb4934",
          "background": "",
          "type": "root"
        },
        {
          "style": "plain",
          "template": " <#458588><b>󱐋</b></><b> {{ .UserName }}</b> <#26C6DA>❯</><#458588>❯</>",
          "foreground": "#b16286",
          "background": "",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
