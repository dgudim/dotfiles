msgid "Analyze"
msgstr "分析"

msgid "Checks"
msgstr "检查"

msgid "Scale To"
msgstr "缩放到"

msgid "Scene Scale"
msgstr "场景缩放"

msgid "Copy Textures"
msgstr "复制材质"

msgid "Volume: {}³"
msgstr "体积: {}³"

msgid "Area: {}²"
msgstr "表面积: {}²"

msgid "Non-manifold Edges: {}"
msgstr "非流形边: {}"

msgid "Bad Contiguous Edges: {}"
msgstr "反向面交界（不良交界边）: {}"

msgid "Intersect Face: {}"
msgstr "交叉面: {}"

msgid "Zero Faces: {}"
msgstr "零面积面: {}"

msgid "Zero Edges: {}"
msgstr "零长度边: {}"

msgid "Non-flat Faces: {}"
msgstr "不平坦面: {}"

msgid "Thin Faces: {}"
msgstr "薄面: {}"

