{{- if eq .chezmoi.hostname "sauron" }}
Sound=/usr/share/sounds/subnautica_theme/session_welcome_long.ogg
{{- else }}
Sound=/usr/share/sounds/subnautica_theme/session_welcome2.ogg
{{- end }}
